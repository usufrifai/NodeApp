{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
"globals": {
    "angular": 1
  },


  "rules": {
"no-console":0,
"no-debugger" :0,
"no-unused-vars":0,
"no-undef":0
  }
}
